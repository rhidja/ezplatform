{% extends "EzPublishCoreBundle::content_fields.html.twig" %}

{% block ramcity_field %}
    {% apply spaceless %}
        <dl {{ block( 'field_attributes' ) }}>
            <dt>Ville</dt>
            <dd>{{ field.value.ville }}</dd>
            <dt>Code Postal</dt>
            <dd>{{ field.value.codePostal }}</dd>
            <dt>Code Insee</dt>
            <dd>{{ field.value.codeInsee }}</dd>
        </dl>
    {% endapply %}
{% endblock %}